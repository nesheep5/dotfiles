[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[user]
	name = Shogo Watanabe
	email = ne.sheep.5mouths@gmail.com
[credential]
	helper = osxkeychain
[core]
	excludesfile = /Users/WatanabeShogo/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
